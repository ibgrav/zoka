---
import { GITHUB_REPO_ORG } from "astro:env/server";
import { GITHUB_REPO_NAME } from "astro:env/server";

const { api } = Astro.locals;

const tree = await api.getGitTree({
  owner: GITHUB_REPO_ORG,
  repo: GITHUB_REPO_NAME,
  sha: "main",
});
---

<div>editor!</div>

<pre>{JSON.stringify(tree, null, 2)}</pre>
