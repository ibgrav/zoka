{
  "name": "zoka",
  "version": "0.0.2",
  "type": "module",
  "files": [
    "dist",
    "jsx-runtime",
    "jsx-dev-runtime"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.cjs"
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "unbuild",
    "test": "vitest run"
  },
  "devDependencies": {
    "mkdist": "^1.0.0",
    "typescript": "4.9.4",
    "unbuild": "1.0.2"
  }
}
