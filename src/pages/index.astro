---
const session = Astro.cookies.get("session")?.value;
const tokens = JSON.parse((session ? await Astro.locals.runtime.env.SESSION.get(session) : null) ?? "{}");
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <title>Astro</title>
  </head>
  <body>
    <a href="/auth/contentful/login">Login with Contentful</a>
    <a href="/auth/storyblok/login">Login with Storyblok</a>
    <pre>{JSON.stringify({ session, tokens }, null, 2)}</pre>
  </body>
</html>
